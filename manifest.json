{

  "manifest_version": 2,
  "name": "IdleExtension",
  "version": "1.0.1",

  "description": "Adds functionality to Melvor Idle",

  "icons": {
    "48": "Icons/icon.png"
  },
  "permissions": [
    "activeTab"
  ],

  "content_scripts": [
    {
      "matches": ["*://melvoridle.com/*"],
      "js": ["jquery-3.5.1.min.js", "extension.js"]
    }
  ]

}
